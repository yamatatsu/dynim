{
	"compilerOptions": {
		"target": "ES2022",
		"module": "NodeNext",
		"lib": ["es2022"],
		"declaration": true,
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"inlineSourceMap": true,
		"inlineSources": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	}
}
